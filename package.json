{
  "name": "candylabsv2",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "17.8.0",
    "npm": "8.5.5"
  },
  "scripts": {
    "start": "node server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch": "run-p serve watch:*",
    "watch:all": "npm run lite",
    "clean": "rimraf dist",
    "imagemin": "imagemin img/* --out-dir='dist/img'",
    "usemin": "usemin /contact.html -d dist --htmlmin -o dist/contact.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin index.html -d dist --htmlmin -o dist/index.html && usemin products.html -d dist --htmlmin -o dist/products.html && usemin gifts.html -d dist --htmlmin -o dist/gifts.html && usemin specials.html -d dist --htmlmin -o dist/specials.html && usemin termsAndConditions.html -d dist --htmlmin -o dist/termsAndConditions.html && usemin privacyPolicy.html -d dist --htmlmin -o dist/privacyPolicy.html",
    "serve": "browser-sync start --server --files --port $PORT \"dist/css/*.css, dist/js/*.js, **/*.html, !node_modules/**/*.html\"",
    "build": "npm run clean && npm run copyfonts && npm run usemin"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cssmin": "^0.4.3",
    "font-awesome": "^4.7.0",
    "grunt": "^1.0.2",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^2.2.1",
    "grunt-contrib-htmlmin": "^2.4.0",
    "grunt-contrib-imagemin": "^2.0.1",
    "grunt-contrib-uglify": "^3.3.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-filerev": "^2.3.1",
    "grunt-sass": "^3.1.0",
    "grunt-usemin": "^3.1.1",
    "htmlmin": "^0.0.7",
    "jit-grunt": "^0.10.0",
    "node-sass": "^7.0.3",
    "onchange": "^3.3.0",
    "rimraf": "^2.6.2",
    "time-grunt": "^1.4.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.5.1"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "bootstrap-social": "^5.1.1",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "jquery": "^3.3.1",
    "lite-server": "^2.6.1",
    "parallelshell": "^3.0.1",
    "popper.js": "^1.12.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TropicalXU/candylabsv2.git"
  },
  "bugs": {
    "url": "https://github.com/TropicalXU/candylabsv2/issues"
  },
  "homepage": "https://github.com/TropicalXU/candylabsv2#readme",
  "description": ""
}
